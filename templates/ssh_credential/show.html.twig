{% extends 'base_dash.html.twig' %}

{% block title %}Credenciales SSH{% endblock %}

{% block content %}
    <h1>Credenciales SSH</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ssh_credential.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ ssh_credential.title }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ ssh_credential.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ ssh_credential.password }}</td>
            </tr>
            <tr>
                <th>Port</th>
                <td>{{ ssh_credential.port }}</td>
            </tr>
        </tbody>
    </table>

    <div class="text-right mt-5">
        <a href="{{ path('ssh_credential_index') }}">Volver</a>
        <a class="btn btn-warning ml-2" href="{{ path('ssh_credential_edit', {'id': ssh_credential.id}) }}"><i class="far fa-edit"></i> Editar</a>
    </div>
{% endblock %}
